# Copy this file to `.env` (or create environment variables in Vercel) and fill values.
# Do NOT commit real secrets to git.

# Supabase
VITE_SUPABASE_URL="https://your-project-ref.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-publishable-key"

# Optional: make the OAuth redirect explicit. If not set, frontend uses window.location.origin + '/chatbot'
# Example for local dev:
# VITE_AUTH_REDIRECT="http://localhost:5173/chatbot"
VITE_AUTH_REDIRECT=""

# n8n / webhook URL used by server functions (if you run n8n or use a webhook-based backend)
VITE_N8N_WEBHOOK_URL="https://your-n8n.example/webhook/your-webhook-id"
